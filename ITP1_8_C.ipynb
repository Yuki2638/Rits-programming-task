   "source": [
    "alp = list(chr(ord(\"a\")+i) for i in range(26))\n",
    "count = [0 for j in range(26)]\n",
    "while True:\n",
    "    try:\n",
    "        word = list(input().lower())\n",
    "    except EOFError:\n",
    "        break\n",
    "    for i in range(26):\n",
    "        count[i] += word.count(alp[i])\n",
    "\n",
    "for i in range(26):\n",
    "    print(alp[i],\":\",count[i])"
   ]
